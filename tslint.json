{
    "extends": [
        "tslint:recommended"
    ],
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "node_modules"
    ],
    "defaultSeverity": "warning",

    "jsRules": {},
    "rules": {
        "max-line-length": {
            "options": [120]
        },
        "ordered-imports": false,
        "trailing-comma": true
    },
    "rulesDirectory": []
}
